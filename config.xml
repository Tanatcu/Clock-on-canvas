<?xml version="1.0" encoding="utf-8" standalone="no"?>
<widget xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0" id="com.coolappz.MyFirstClock" version="1.0.0">
    <name>Clock</name>
    <description>
       The first test application framework, written in phonegap.
    </description>
    <author email="ruslan.iuko777@mail.ru" href="https://vk.com/tanatcu">
        Ruslan
    </author>
    <content src="index.html"/>
    <plugin name="org.apache.cordova.battery-status"/>
    <access origin="*"/>
    <allow-intent href="http://*/*"/>
    <allow-intent href="https://*/*"/>
    <allow-intent href="tel:*"/>
    <allow-intent href="sms:*"/>
    <allow-intent href="mailto:*"/>
    <allow-intent href="geo:*"/>
    <platform name="android">
        <allow-intent href="market:*"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*"/>
        <allow-intent href="itms-apps:*"/>
    </platform>
</widget>
